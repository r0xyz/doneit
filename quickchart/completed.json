        "labels": [
          {
            "text": "\n達成率",
            "color": "#aaa",
            "font": {
              "size": "25"
            }
          },
          {
            "text": "\n$SUCCEED_RATE%",
            "font": {
              "size": "40"
            }
          }
        ]
      }
    }
  }
}